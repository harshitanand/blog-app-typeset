{
  "name": "typeset-task",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node .",
    "posttest": "npm run lint && nsp check",
    "prettier": "prettier --single-quote --trailing-comma es5 --print-width 120 --write"
  },
  "lint-staged": {
    "client/app/**/*.js": [
      "eslint -c client/app/.eslintrc --ignore-path client/app/.eslintignore",
      "git add"
    ],
    "*.js": [
      "eslint --ignore-pattern client/js",
      "npm run prettier",
      "git add"
    ]
  },
  "dependencies": {
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "helmet": "^1.3.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.0.0",
    "path": "^0.12.7",
    "prettier": "^1.5.3",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-loopback": "^8.0.0",
    "eslint-config-prettier": "^1.7.0",
    "nsp": "^2.1.0",
    "prettier": "1.4.2"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "MIT",
  "description": "typeset-task",
  "author": "Harshit Anand <harshitanand94@gmail.com>"
}
